/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a=sel,b=sel,out=m);
    And(a=a,b=m,out=x);
    And(a=b,b=sel,out=y);
    Nand(a=x,b=x,out=p);
    Nand(a=y,b=y,out=q);
    Nand(a=p,b=q,out=out);
}